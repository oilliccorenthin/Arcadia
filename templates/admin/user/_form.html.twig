{{ form_start(form) }}
    <div class="row">
        <div class="col-md-4">{{ form_row(form.email) }}</div>
        <div class="col-md-4">{{ form_row(form.password) }}</div>
        <div class="col-md-4">{{ form_row(form.roleObjects) }}</div>
    </div>
        {{ form_rest(form) }}
        <button class="btn btn-primary">{{button|default('Enregistrer')}}</button>
{{ form_end(form) }}